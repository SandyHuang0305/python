# 九九乘法表
#方法1
for i in range(1, 10 ):
    for j in range(1, i+1):
        if i == j: #i = j 的部分要處理
            print(j, '*', i, '=', i*j, end='\n')
        else:
            print('%d*%d=%2d' % (j,i,i*j),end='\t')    

#方法2
for i in range(1,10):
    for j in range(1, i+1):
        print('%d*%d=%2d' % (j,i,i*j), end='\t')
    print()    
